{
  "name": "XnD's-Elden-Compendium",
  "description": "Elden Ring Compendium app",
  "author": "XnD",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "main.js",
  "build": {
    "appId": "XnDeldenCompendium",
    "productName": "Xnd's Elden Compendium",
    "files": [
      "dist/**",
      "node_modules/**",
      "public/**",
      "main.js",
      "preload.cjs",
      "package.json"
    ],
    "directories": {
      "output": "release",
      "buildResources": "build"
    },
    "win": {
      "target": "portable",
      "icon": "icon.ico"
    }
  },
  "scripts": {
    "dev": "vite",
    "devhost":"vite --host",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron": "electron .",
    "dist": "electron-builder"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "antd": "^5.25.4",
    "electron-squirrel-startup": "^1.0.1",
    "i18next": "^25.2.1",
    "lodash.debounce": "^4.0.8",
    "puppeteer": "^24.10.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.6.2",
    "recharts": "^3.0.0",
    "scss": "^0.2.4",
    "sharp": "^0.34.2",
    "vite-plugin-checker": "^0.9.3"
  },
  "devDependencies": {
    "@electron/fuses": "^1.8.0",
    "@eslint/js": "^9.25.0",
    "@types/lodash.debounce": "^4.0.9",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "electron": "^36.5.0",
    "electron-builder": "24.6.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "sass": "^1.89.1",
    "sass-embedded": "^1.89.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
